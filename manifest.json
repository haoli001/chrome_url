{
        "manifest_version": 2,
        "name": "信辉神，无bug",
        "version": "0.2.1",
        "description": "orz_orz_orz_orz",
        "icons": {
        },
        "browser_action": {
            "default_title": "url",
            "default_popup": "popup.html"
        },
        "icons": { "16": "icon-16.png", "128": "icon-128.png" },
        "permissions": [
        "contextMenus",
        "tabs"
        ],
        "background": { 
            "scripts": ["menu.js"] ,
            "persistent": true
        }, 
        "commands": {
            "toggle-feature-foo": {
                "suggested_key": {
                                  "default": "Ctrl+Shift+Y",
                                                "mac": "Command+Shift+Y"
                                                            
                },
                            "description": "Toggle feature foo"
                                      
            },
            "_execute_browser_action": {
                "suggested_key": {
                                  "windows": "Ctrl+Shift+Y",
                                                "mac": "Command+Shift+Y",
                                                              "chromeos": "Ctrl+Shift+U",
                                                                            "linux": "Ctrl+Shift+J"
                                                                                        
                }
                          
            },
            "_execute_page_action": {
                "suggested_key": {
                                  "default": "Ctrl+Shift+E",
                                                "windows": "Alt+Shift+P",
                                                              "mac": "Alt+Shift+P"
                                                                          
                }
                          
            }
                    
        },
        "options_page": "options.html"

}
